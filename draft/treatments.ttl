d:ParacetamolPrescription a s:MedicalTherapy ;
  s:drug drugs:Paracetamol ;
  s:doseSchedule [
    s:doseUnit "mg" ;
    s:doseValue "1000"^^xsd:unsignedInt ;
    s:frequency "every 6 hours"@en, "toutes les 6 heures"@fr .
  ] .

d:IbuprofenePrescription a s:MedicalTherapy ;
  s:drug drugs:Ibuprofene ;
  s:doseSchedule [
    s:doseUnit "mg" ;
    s:doseValue "1000"^^xsd:unsignedInt ;
    s:frequency "every 8 hours"@en, "toutes les 8 heures"@fr .
  ] .

d:AmoxicillinPrescription a s:MedicalTherapy ;
  s:drug drugs:Amoxicillin ;
  s:doseSchedule [
    s:doseUnit "mg" ;
    s:doseValue "1000"^^xsd:unsignedInt ;
    s:frequency "morning and night"@en, "matin et soir"@fr .
  ] .

d:MetforminPrescription a s:MedicalTherapy ;
  s:drug drugs:Metformin ;
  s:doseSchedule [
    s:doseUnit "mg" ;
    s:doseValue "500"^^xsd:unsignedInt ;
    s:frequency "morning and night"@en, "matin et soir"@fr .
  ] .

d:InsulinPrescription a s:MedicalTherapy ;
  s:drug drugs:Insulin ;
  s:doseSchedule [
    s:doseUnit "UI" ;
    s:doseValue "1"^^xsd:unsignedInt ;
    s:frequency "0.2 UI / kg"@en, "0.2 UI / kg"@fr .
  ] .

d:TherapeuticEducation a s:MedicalTherapy ;
  s:name "Therapeutic Education"@en, "Education thérapeutique"@fr

d:BalancedDiet a s:MedicalTherapy ;
